<?xml version="1.0" encoding="UTF-8"?>
<model name="scim:scimModel"
       xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>SCIM Model</description>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
    </imports>
    <namespaces>
        <namespace uri="http://jrudinskij.lt/model/scim/1.0" prefix="scim"/>
    </namespaces>
    <aspects>
        <aspect name="scim:ExternalAuthority">
            <properties>
	            <property name="scim:externalId">
	                <type>d:text</type>
	            </property>
	            <property name="scim:lastSyncDate">
	                <type>d:date</type>
	            </property>
            </properties>
        </aspect>
    </aspects>
</model>